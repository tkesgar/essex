{
  "name": "tkesgar.com",
  "version": "1.2.1",
  "description": "Personal website repository",
  "private": true,
  "author": "Ted Kesgar <t.kesgar@live.com> (https://tkesgar.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkesgar/tkesgar.com.git"
  },
  "homepage": "https://github.com/tkesgar/tkesgar.com#readme",
  "scripts": {
    "dev": "next",
    "build": "next build && next export",
    "start": "serve out",
    "test": "jest",
    "lint": "xo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "bootswatch": "^4.2.1",
    "next": "^7.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "reactstrap": "^7.0.2",
    "sharo-scripts": "^0.6.0"
  },
  "devDependencies": {
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "xo": "^0.23.0"
  },
  "babel": {
    "presets": [
      [
        "next/babel",
        {
          "styled-jsx": {
            "plugins": [
              "styled-jsx-plugin-postcss"
            ]
          }
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 1 major version",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "xo": {
    "extends": [
      "xo-react"
    ],
    "parser": "babel-eslint",
    "space": 2,
    "semicolon": false,
    "settings": {
      "react": {
        "version": "16.6.0"
      },
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".md",
            ".mdx",
            ".scss"
          ]
        }
      }
    }
  },
  "stylelint": {
    "extends": [
      "sharo-scripts/stylelint"
    ]
  }
}
